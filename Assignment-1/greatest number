#include <bits/stdc++.h>
using namespace std;
int main() {
int a,b,c,greatest;
cout<<"enter the first number "<<endl;
cin>>a;
cout<<"enter the second number "<<endl;
cin>>b;
cout<<"enter the third number "<<endl;
cin>>c;
greatest=a;
if(b>greatest)
    {greatest=b;}
if(c>greatest)
    {greatest=c;}
cout<<"greatest number is ";
cout<<greatest;
return 0;
}
